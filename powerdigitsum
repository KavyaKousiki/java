package com.example;
import java.math.*;
public class Powerdigitsum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int m=0;
        int s=(int)Math.pow(2,1000);
        while(s!=0)
        {
        	int r=s%10;
        	m=m+r;
        	s=s/10;
        	
        }
         System.out.println(m);
	}

}
