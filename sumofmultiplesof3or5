package com.example;

public class Sumofmultiples3or5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       int i,j,sum=0;
       for(i=3;i<1000;i++)
       {
    	   if(i%3==0 || i%5==0)
    	   {
    		   sum=sum+i;
    	   }
       }
       System.out.println(sum);
       
	}

}
